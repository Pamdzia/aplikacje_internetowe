<!DOCTYPE html>
<html lang="pl">
<head>
  <meta charset="UTF-8">
  <title>Zakończone przetargi</title>
  <link rel="stylesheet" href="/css/closed-tenders.css">
</head>
<body>
  <div class="navbar">
    <a href="/">Strona główna</a>
    <a href="/tenders">Lista przetargów</a>
    <a href="/closed-tenders">Lista zakończonych przetargów</a>
    <a href="/add-tender">Dodaj przetarg</a>
  </div>

  <h1>Zakończone przetargi</h1>
  <table align="center">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Nazwa Przetargu</th>
        <th scope="col">Status</th>
      </tr>
    </thead>
    <tbody>
      <% tenders.forEach((tender, index) => { %>
        <tr>
          <th scope="row"><%= index + 1 %></th>
          <td><a href="/tender-offers/<%= tender.IDPrzetargu %>"><%= tender.Nazwa %></a></td>
          <td>Przetarg zakończony</td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
